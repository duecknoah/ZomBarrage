        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Wait for obj_roomObjectsInit to init this
/* Wait for obj_roomObjectsInit to order the object creation
 * from top left to bottom right, once that sets this timeToSpawn,
 * this object will then spawn the building parts when ordered, then delete itself
 */
if (timeToSpawn) {
    // Create the building, then destroy self
    // Roof
    /* Adds the specific tiles to the room / map,
     * scr_getDepth() automatically sets the depth
     * of the tiles based on y height. The lower the
     * number, the closer to the camera. -2 is used
     * to make the tile seem above the player as it
     * is a roof
     */
    // Based on bench direction, create tiles accordingly
    switch (benchDir) {
        case "up":
            scr_addBackTile(40, x, y, 0, 0, scr_getDepth(y, 0, true));
            // Collisions
            with (instance_create(x, y, obj_collision)) {
                x += back_tile_width / 4;
                y += back_tile_height / 2;
                image_xscale /= 2;
                image_yscale /= 4;  
            }
        break;
        case "down":
            scr_addBackTile(41, x, y, 0, 0, scr_getDepth(y, 0, true));
            // Collisions
            with (instance_create(x, y, obj_collision)) {
                x += back_tile_width / 3;
                y += back_tile_height / 2;
                image_xscale /= 2;
                image_yscale /= 4; 
            }
        break;
        case "left":
            scr_addBackTile(43, x, y, 0, 0, scr_getDepth(y, 0, true));
            // Collisions
            with (instance_create(x, y, obj_collision)) {
                x += back_tile_width / 2;
                y += back_tile_height / 2;
                image_xscale /= 4;
                image_yscale /= 2;  
            }
        break;
        case "right":
            scr_addBackTile(42, x, y, 0, 0, scr_getDepth(y, 0, true));
            // Collisions
            with (instance_create(x, y, obj_collision)) {
                x += back_tile_width / 2;
                y += back_tile_height / 2;
                image_xscale /= 4;
                image_yscale /= 2;   
            }
        break;
    }
    instance_destroy(); 
}
</string>
