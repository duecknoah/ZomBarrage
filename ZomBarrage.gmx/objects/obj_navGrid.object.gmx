            <kind>1</kind>
            <string>/// Frequently update navUpdateRateTimer for ai

// check for new collision objects to add to nav grid
if (collisionObjectCount != instance_number(obj_collision)) {
    mp_grid_add_instances(global.grid, obj_collision, false);
    collisionObjectCount = instance_number(obj_collision);
}

if (global.navUpdateRateTimer &lt; 0) {
    global.navUpdateRateTimer = global.navUpdateRate;
    mp_grid_clear_all(global.grid);
    if (instance_exists(obj_collision)) {
        mp_grid_add_instances(global.grid, obj_collision, false);
    }
    if (instance_exists(obj_zombieP)) {
        mp_grid_add_instances(global.grid, obj_zombieP, false);
    }
    /*
    if (instance_exists(obj_zombie)) {
        mp_grid_add_instances(global.grid, obj_zombie, false);
    }
    if (instance_exists(obj_heavyZombie)) {
        mp_grid_add_instances(global.grid, obj_bigZombie, false);
    }
    */
    if (instance_exists(obj_civilian)) {
        mp_grid_add_instances(global.grid, obj_civilian, false);
    }  
}
else {
    global.navUpdateRateTimer --;
}
